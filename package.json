{
  "name": "giscus.app",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "cscript": "tsc client.ts --outDir public",
    "cthemes": "postcss styles/themes/*.css --dir public/themes --config styles/themes",
    "mscript": "google-closure-compiler < public/client.js --js_output_file public/client.js",
    "dev": "yarn cscript --watch --preserveWatchOutput & yarn cthemes --watch & next dev",
    "build": "yarn cscript && yarn cthemes && yarn mscript && next build",
    "start": "next start",
    "lint": "stylelint styles/**/*.css && eslint . --cache --max-warnings 0",
    "lint:fix": "stylelint --fix styles/**/*.css && eslint --fix . --cache --max-warnings 0",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@primer/octicons-react": "^19.14.0",
    "dompurify": "^3.2.4",
    "jsonwebtoken": "^9.0.3",
    "lit-html": "^3.2.1",
    "mathjax": "^3.2.0",
    "next": "^14.2.32",
    "next-translate": "^2.6.2",
    "next-translate-plugin": "^2.6.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "swr": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.39.2",
    "eslint-config-next": "^14.2.32",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "google-closure-compiler": "^20260121.0.0",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-vanilla-rtl": "^0.4.0",
    "typescript": "^5.7.2"
  },
  "resolutions": {
    "brace-expansion": "2.0.2",
    "braces": "3.0.3",
    "cross-spawn": "7.0.5",
    "glob": "10.5.0",
    "js-yaml": "4.1.1",
    "nanoid": "3.3.8",
    "ws": "7.5.10"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
